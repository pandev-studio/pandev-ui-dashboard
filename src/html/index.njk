{% extends "layouts/base.njk" %}

{% set title = "Index page" %}

{% set pages = [ 'page1', 'page2', 'page3' ] %}

{% block content %}
    <h1 class="js-suffix">Pages</h1>
    <div class="plus-one"></div>
    <ul>
        <li><a href="/styleguide">Styleguide</a></li>
        {% for path in pages %}
            <li><a href="{{ path }}.html">{{ path }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}